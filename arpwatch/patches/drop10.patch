+
+static void go_daemon()
+{
+	pid_t pid;
+        int fd;
+
+	pid = fork();
+	if(pid < 0) {
+		syslog(LOG_ERR, "main fork(): %m");
+		exit(1);
+	} else if(pid != 0) {
+		exit(0);
+	}
+
+	close(fileno(stdin));
+	close(fileno(stdout));
+	close(fileno(stderr));
+#ifdef TIOCNOTTY
+	fd = open("/dev/tty", O_RDWR);
+	if(fd >= 0) {
+		ioctl(fd, TIOCNOTTY, 0);
+		close(fd);
+	}
+#else
+	setsid();
+#endif
+
+}
