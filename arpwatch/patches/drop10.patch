+{
+	struct passwd* pw;
+	pw=getpwnam(user);
+	if(pw) {
+		if( initgroups(pw->pw_name, 0) != 0 ||
+		    setgid(pw->pw_gid) != 0 ||
+		    setuid(pw->pw_uid) != 0
+		  ){
+			fprintf(stderr, "%s: could not change to %.32s uid=%d gid=%d; exiting", prog, user,pw->pw_uid, pw->pw_gid);
+			exit(1);
+		}
+
+	} else {
+		fprintf(stderr, "%s: could not find user '%.32s'; exiting\n", prog, user);
+		exit(1);
+	}
+
