+int setup_reportmode(int mode)
+{
+	int ret=0;
+
+	switch(mode) {
+
+	case REPORT_NORMAL:
+		report_f=report_orig;
+		break;
+
+	case REPORT_STDOUT:
+		report_f=report_stdout;
+		break;
+
+	case REPORT_RAW:
+		report_f=report_raw;
+		break;
+
+	default:
+		report_f=report_orig;
+		ret=1;
+	}
+
+	return ret;
+}
