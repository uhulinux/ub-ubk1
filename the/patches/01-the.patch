+void encodeqp(FILE *infile, FILE *outfile, int linesize)
+{
+  unsigned char in;
+  int len = 1;
+  while (!feof(infile))
+  {
+    in = (unsigned char) getc(infile);
+    if (!feof(infile))
+      {
+      if (len >= linesize)
+      {
+        fprintf(outfile, "=\r\n");
+        len = 1;
+      }
+      if (((in>0x1F && in<0x80) && in!=0x3d)
+          || (in==0x0a || in==0x0d))
+      {
+        putc(in, outfile);
+        len++;
+        if (in==0x0a || in==0x0d)
+          len = 1;
+      }
+      else
+      {
+        if (len >= (linesize-3))
+        {
+          fprintf(outfile, "=\r\n");
+          len = 1;
+        }
+        fprintf(outfile, "=%02X", in);
+        len += 3;
+      }
+    }
+  }
