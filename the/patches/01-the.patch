+          }
+          else
+          {
+            out = 16*h2b(in);
+            in = getc(infile);
+            out += h2b(in);
+            putc(out, outfile);
+          }
+        }
+      }
+      else
+      {
+        putc(in, outfile);
+      }
+    }
+  }
