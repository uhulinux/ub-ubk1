+#include <assert.h>
+
+int sha1proc (char *fn, char *buf)
+{
+  assert (sizeof (u32) == 4);
+  FILE *fp;
+  char buffer[4096];
+  size_t n;
+  SHA1_CONTEXT ctx;
+  int i;
+  
+  fp = fopen(fn, "rb");
+  if (!fp)
+  {
+    return(1);
+  }
+  sha1_init(&ctx);
+  while ( (n = fread(buffer, 1, sizeof buffer, fp)))
+    sha1_write(&ctx, buffer, n);
+  if (ferror(fp))
+  {
+    return(1);
+  }
+  sha1_final (&ctx);
+  fclose(fp);
+  for (i=0; i < 20; i++)
+    sprintf(buf+(2*i), "%02x", ctx.buf[i]);
+  sprintf(buf+40, "  \0");
+  return 0;
+}
