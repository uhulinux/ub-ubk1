+static ftype fun_prec(ftype a)
+{
+  unsigned short dec;
+  char *fstr = form;
+  dec = (unsigned short) a;
+  if (dec<PREC_NUM)
+  {
+    fstr += 2;
+    sprintf(fstr, "%02d", dec);
