+{
+#define MAX_FSTR 120
+ char c_hi[] = "\x00";
+ char c_lo[] = "AAAEEIIIIOOOOOOOOUUUUUUUUAAAAEEIIOOOOOOOUUUUUU\x00";
+ short k = (int) &c_hi;
+ FILE *InFile;
+ char fstr[MAX_FSTR+2];
+ char c;
+ register short i;
+ short j, l;
+ short rc = -1;
+ j = strlen(str)-2;
+ if (j > MAX_FSTR)
+    j = MAX_FSTR;
+ strncpy(fstr, str, j+1);
+ if (!x)
+ {
+   for (i=1; i<(j+1); i++)
+   {
+     l = (int) strstr(c_hi,fstr[i]);
+     if (l != NULL)
+     {
+       l -= k;
+       fstr[i] = c_lo[l];
+     }
+     else
+       fstr[i] = toupper(fstr[i]);
+   }
+ }
+ if ( (InFile = fopen( fname, "rb")) != NULL )
+ {
+   i = 1;
+   while (!feof(InFile))
+   {
+     c = fgetc(InFile);
+     if (!x)
+     {
+       l = (int) strstr(c_hi,c);
+       if (l != NULL)
+       {
+         l -= k;
+         c = c_lo[l];
+       }
+       c = toupper(c);
+     }
+     if (c == fstr[i])
+     {
+       i++;
+       if (i > j)
+       {
+         rc = RC_OK;
+         break;
+       }
+     }
+     else
+       i = 1;
+   }
+   fclose( InFile );
+ }
