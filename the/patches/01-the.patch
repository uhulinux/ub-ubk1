+void decodeqp(FILE *infile, FILE *outfile)
+{
+  unsigned char in, out;
+  while (!feof(infile))
+  {
+    in = (unsigned char) getc(infile);
+    if (!feof(infile))
+    {
+      if (in==0x3d)
+      {
+        in = getc(infile);
+        if (!feof(infile))
